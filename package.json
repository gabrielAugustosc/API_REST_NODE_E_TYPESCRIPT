{
  "name": "api-rest-node-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "author": "Gabriel Augusto",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "ts-node-dev ./src/index.ts",
    "postinstall": "tsc",
    "test": "jest",
    "knex:migrate": "knex --knexfile ./src/server/database/Environment.ts migrate:latest",
    "knex:rollback": "knex --knexfile ./src/server/database/Environment.ts migrate:rollback",
    "knex:rollback:all": "knex --knexfile ./src/server/database/Environment.ts migrate:rollback --all",
    "knex:seed": "knex --knexfile ./src/server/database/Environment.ts seed:run"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "http-status-codes": "^2.3.0",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.1.0",
    "@types/supertest": "^6.0.3",
    "@vercel/node": "^5.5.28",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "engines": {
    "node": ">=16.x"
  }
}
